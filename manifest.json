{
  "manifest_version": 2,
  "name": "colorAWSFrame",
  "description": "Change background color of header & footer on AWS Management Console depending on role name or account.",
  "version": "1.1",
  "content_scripts": [
    {
      "matches": [
        "http://*.aws.amazon.com/*",
        "https://*.aws.amazon.com/*"
      ],
      "js": [
        "js/chame.js",
        "js/colorAWSFrame.js"
      ]
    }
  ],
  "options_ui": {
    "page": "options/settings.html",
    "open_in_tab": true
  },
  "permissons": [
    "storage"
  ]
}